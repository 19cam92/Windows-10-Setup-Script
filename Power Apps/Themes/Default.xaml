                    xmlns:local="clr-namespace:Power_Apps.Themes">
    
    <SolidColorBrush x:Key="Color.Window.Background" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="Color.Window.Foreground" Color="#000000"/>

    <Color x:Key="Color.HamburgerButton.Background">#353535</Color>
    <Color x:Key="Color.HamburgerButton.Hover">#626262</Color>
    <SolidColorBrush x:Key="Color.HamburgerButton.Foreground">#FFFFFF</SolidColorBrush>
    <SolidColorBrush x:Key="Color.HamburgerButton.Disabled">#848484</SolidColorBrush>

    <SolidColorBrush x:Key="Color.HamburgerSelector.Background">#0078d7</SolidColorBrush>    

    <SolidColorBrush x:Key="Color.Indicator.ThreeDots.Background">Transparent</SolidColorBrush>
    <SolidColorBrush x:Key="Color.Indicator.ThreeDots.Ellipse.Background">#353535</SolidColorBrush>
    <SolidColorBrush x:Key="Color.Indicator.ThreeDots.Text.Foreground">#353535</SolidColorBrush>

</ResourceDictionary>